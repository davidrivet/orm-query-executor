<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
<class name="com.jpaqueryexecutor.entity.Employee" table="employee">
<id name="eid" type="int" column="eid"><generator class="native"/></id>
<property name="ename" type="string" not-null="true" />
<property name="salary" type="double" not-null="true" />
<property name="deg" type="string" not-null="true" />
</class>
</hibernate-mapping>
